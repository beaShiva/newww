<%- include("partials/header"); -%>



<div class="container contact">
    
    <div class="row">
        <div class="col-lg-4 col-sm-4">
            <img src="css/contact.jpg" alt="">
        </div>
        <div class="col-lg-8 col-sm-8 main-contact">
         <h2 class="contact-heading">Contact me</h2>

         <form action="/contact" method="post">

         <input  class=input-contact  type="text" name="name" placeholder="Your name">
         <input class=input-contact  type="email" name="email" placeholder="Your Email">
         <textarea  class=input-contact placeholder="Any suggestion !" name="textarea" id="" cols="90" rows="5"></textarea><br>
        <button class=" btn-primary btn btn-lg contact-btn" type="submit">Send</button>
    </form>
        </div>
    </div>
</div>






<%- include("partials/footer"); -%>